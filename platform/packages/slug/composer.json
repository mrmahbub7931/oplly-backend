{
    "name": "canopy/slug",
    "description": "Slug/Permalink for Canopy CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Canopy\\Slug\\": "src"
        }
    },
    "require": {
        "canopy/platform": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Canopy\\Slug\\Providers\\SlugServiceProvider"
            ],
            "aliases": {
                "SlugHelper": "Canopy\\Slug\\Facades\\SlugHelperFacade"
            }
        }
    }
}
